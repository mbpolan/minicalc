.obj/mainwindow.o: mainwindow.cpp mainwindow.h \
		calcbody.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/mainwindow.o mainwindow.cpp

.obj/calcbody.o: calcbody.cpp calcbody.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/calcbody.o calcbody.cpp

.obj/buttons.o: buttons.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/buttons.o buttons.cpp

.obj/moc_calcbody.o: .moc/moc_calcbody.cpp calcbody.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_calcbody.o .moc/moc_calcbody.cpp

.obj/moc_buttons.o: .moc/moc_buttons.cpp buttons.h 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o .obj/moc_buttons.o .moc/moc_buttons.cpp

.moc/moc_calcbody.cpp: $(MOC) calcbody.h
	$(MOC) calcbody.h -o .moc/moc_calcbody.cpp

.moc/moc_buttons.cpp: $(MOC) buttons.h
	$(MOC) buttons.h -o .moc/moc_buttons.cpp

